<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="401.0" layoutX="1.0" layoutY="-1.0" pickOnBounds="true">
         <image>
            <Image url="@../../../../bibliotheque-1623.jpg" />
         </image>
      </ImageView>
      <Label layoutX="520.0" layoutY="7.0" text="Login">
         <font>
            <Font size="64.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="499.0" layoutY="197.0" prefHeight="45.0" prefWidth="230.0" promptText="username" />
      <PasswordField fx:id="password" layoutX="506.0" layoutY="286.0" prefHeight="45.0" prefWidth="230.0" promptText="password" />
      <Button fx:id="bt" layoutX="567.0" layoutY="372.0" mnemonicParsing="false" onAction="#login" text="entrer" />
   </children>
</AnchorPane>
